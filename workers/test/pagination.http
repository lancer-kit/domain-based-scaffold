# get the list of obj
GET http://localhost:2490/dev/AllBuzz
Accept: */*
Content-Type: application/json
Jwt: {"jti":"1"}
###
# get the list of obj from the end
GET http://localhost:2490/dev/AllBuzz?order=desc
Accept: */*
Content-Type: application/json
Jwt: {"jti":"1"}
###
# get the first 10 obj
GET http://localhost:2490/dev/AllBuzz?pageSize=10
Accept: */*
Content-Type: application/json
Jwt: {"jti":"1"}
###
# get 10 obj on second page
GET http://localhost:2490/dev/AllBuzz?pageSize=10&page=2
Accept: */*
Content-Type: application/json
Jwt: {"jti":"1"}
